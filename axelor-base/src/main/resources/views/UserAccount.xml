  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.2.xsd">

  <grid name="user-account-data-grid" title="User Account"
    model="com.axelor.apps.base.db.UserAccount">

    <field name="userEmail" title="User Email"/>
    <field name="password" title="Password"/>
    <field name="isActive" title="Is Active"/>

