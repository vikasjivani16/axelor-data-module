<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.2.xsd">

  <module name="online" package="com.axelor.apps.online.db"/>

  <entity name="OrderLine">

    <date name="orderDate" title="Order Date" required="true"/>
    <string name="clothSize" title="Size" selection="online.order.line.size"/>
    <decimal name="orderQty" title="Order Qty"/>
    <string name="note" title="Note"/>

    <many-to-one ref="com.axelor.apps.online.db.Cloth" name="cloth" title="Cloth"/>

  </entity>

</domain-models>
